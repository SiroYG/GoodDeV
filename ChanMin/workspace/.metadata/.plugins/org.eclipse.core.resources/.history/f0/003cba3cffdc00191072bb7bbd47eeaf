<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dev.cloud.dao.historyMapper">
<insert parameterType="Total" id="insertHistory">
		insert into History (
			HistoryNum
			,itemNum
			,comments
			,itemVersion
			,historyDate
			)
		VALUES (History_seq.NEXTVAL
				,#{itemNum}
				,#{comments}
				,#{itemVersion}
				,#{historyDate}
				)
</insert>

<select parameterType="Total" id="selectAllHistory" resultType="History">
	select 
		*
	from 
		History
	where 
		itemNum=#{itemNum}
</select>
</mapper>


