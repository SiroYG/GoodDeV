<mapper namespace="com.dev.cloud.dao.documentMapper">
	
<insert parameterType="Document" id="insertDocument">
insert into 
		Document (
					DocumentNum
					,PatentsubNum 
<if test="documentFilename!=''">
					,documentFilename
					,saveDocumentFilename 
</if>
					)VALUES (
					Document_seq.NEXTVAL
					,#{PatentsubNum} 
<if test="documentFilename!=''">
					,#{documentFilename}
					,#{saveDocumentFilename}
</if>
) 
</insert>

<update parameterType="Document" id="updateDocument">UPDATE DocumentSETdocumentFilename = #{documentFilename},saveDocumentFilename = #{saveDocumentFilename}whereDocumentNum=#{DocumentNum} </update>
</mapper>


