<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dev.cloud.dao.patentsubMapper">
	
<insert parameterType="Patentsub" id="insertPatentsub">
insert into 
		Patentsub (
					PatentsubNum
					,patentNum
					,memberId 
<if test="referenceFilename!=''">
					,referenceFilename
					,saveReferenceFilename 
</if>
					) VALUES (
					Patentsub_seq.NEXTVAL
					,#{patentNum}
					,#{memberId} 
<if test="referenceFilename!=''">
					,#{referenceFilename}
					,#{saveReferenceFilename} 
</if>
					) 
</insert>

<update parameterType="Patentsub" id="updatePatentsub">
UPDATE Patentsub
		SET
		referenceFilename = #{referenceFilename}
		,saveReferenceFilename = #{saveReferenceFilename}
		wherePatentsubNum=#{PatentsubNum}
</update>

<select id="selectPatentsub" resultType="Patentsub" parameterType="Patentsub">
select 
		*
from 		
	Patentsub
where
	patentNum = #{patentNum}	
</select>


</mapper>


