<mapper namespace="com.dev.cloud.dao.documentMapper">
	
<insert parameterType="ItemDo" id="insertDocument">
insert into 
		Document (
					documentNum
					,patentsubNum 
					,itemNum
<if test="documentFilename!=null">
					,documentFilename
					,saveDocumentFilename 
</if>
					)VALUES (
					Document_seq.NEXTVAL
					,#{patentsubNum}
					,#{itemNum} 
<if test="documentFilename!=null">
					,#{documentFilename}
					,#{saveDocumentFilename}
</if>
) 
</insert>

<update parameterType="ItemDo" id="updateDocument">
UPDATE Document
	SET
	documentFilename = #{documentFilename}
	,saveDocumentFilename = #{saveDocumentFilename}
where
	DocumentNum=#{DocumentNum} 
</update>

<update id="updateDocu" parameterType="PdpVo" >
UPDATE Document
	SET
	documentFilename = #{documentFilename}
	,saveDocumentFilename = #{saveDocumentFilename}
where
	DocumentNum=#{DocumentNum} 

</update>



</mapper>


