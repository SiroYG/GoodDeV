<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dev.cloud.dao.PTIMapper">
<insert parameterType="ItemDo" id="insertPTI">
insert into PTI (
				pti_seq
				,itemNum
				,patentNum
				,contract
				,contractDate
				)VALUES (
				PTI_seq.NEXTVAL
				,#{itemNum}
				,#{patentNum}
				,'wait'
				,SYSDATE
				) 
				</insert>

<update parameterType="ItemDo" id="updatePTI">
UPDATE PTI
	SET
	contract = #{contract}
where
	PTI_seq=#{PTI_seq} 
</update>



</mapper>


